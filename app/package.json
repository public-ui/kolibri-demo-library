{
	"name": "@public-ui/library-app",
	"description": "This app contains samples for the KoliBri/Public UI",
	"version": "0.0.0",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "rimraf dist && cross-env NODE_ENV=production webpack",
		"build:deps": "pnpm --filter @public-ui/library-app^... build",
		"format": "prettier --check src",
		"lint": "eslint \"{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}\"",
		"serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map",
		"start": "npm run serve -- --open",
		"unused": "ts-prune -e src",
		"prepare": "npm-run-all prepare:*",
		"prepare:components-assets": "cpy \"node_modules/@public-ui/components/assets/**/*\" public/assets --dot",
		"prepare:themes-assets": "cpy \"node_modules/@public-ui/themes/assets/**/*\" public/assets --dot"
	},
	"dependencies": {
		"@leanup/stack": "1.3.49",
		"@leanup/stack-react": "1.3.49",
		"@leanup/stack-webpack": "1.3.49",
		"@public-ui/components": "2.0.6",
		"@public-ui/library-components": "0.0.0",
		"@public-ui/library-react": "0.0.0",
		"@public-ui/react": "2.0.6",
		"@types/node": "20.11.19",
		"@types/react": "18.2.57",
		"@types/react-dom": "18.2.19",
		"@unocss/preset-uno": "0.58.5",
		"@unocss/webpack": "0.58.5",
		"ajv": "8.12.0",
		"chromedriver": "121.0.2",
		"cpy-cli": "5.0.0",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-react": "7.33.2",
		"formik": "2.4.5",
		"nightwatch-axe-verbose": "2.3.0",
		"npm-run-all": "4.1.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router": "6.22.1",
		"react-router-dom": "6.22.1",
		"rimraf": "3.0.2",
		"ts-prune": "0.10.3",
		"typescript": "5.3.3",
		"world_countries_lists": "2.9.0",
		"yup": "1.3.3"
	},
	"files": [
		".eslintignore",
		".eslintrc.js",
		".gitignore",
		".prettierignore",
		"dist/**",
		"prettier.config.js",
		"public/**",
		"src/**",
		"test-build.sh",
		"tsconfig.json",
		"unocss.config.ts",
		"webpack.config.js"
	]
}
